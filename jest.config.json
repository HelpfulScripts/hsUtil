{
    "collectCoverage": true,
    "coverageDirectory": "./_dist/coverage/",
    "coverageReporters": ["json", "lcov", "text", "html"],
    "rootDir": "./",
    "roots":["./src"],
    "testMatch": ["**/*.jest.ts"],
    "moduleFileExtensions": [
        "ts",
        "tsx",
        "js",
        "jsx",
        "json"
    ],
    "transform": {
        "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testURL": "http://localhost/",
    "verbose": false
}